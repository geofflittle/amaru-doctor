{
  "keybindings": {
    "Home": {
      "Esc": "Quit", // Quit
      "<Ctrl-d>": "Quit", // Another way to quit
      "<Ctrl-c>": "Quit", // Yet another way to quit
      "<Ctrl-z>": "Suspend", // Suspend the application
      "<Up>": "ScrollUp",
      "<Down>": "ScrollDown",
      "<Shift-Up>": "FocusUp",
      "<Shift-Down>": "FocusDown",
      "<Shift-Left>": "FocusLeft",
      "<Shift-Right>": "FocusRight",
      "k": "FocusUp",
      "j": "FocusDown",
      "h": "FocusLeft",
      "l": "FocusRight",
    },
  }
}
